# alias here                                                                                                             
alias rm="rm -i"                                                                                                         
alias ll="ls -ltr"                                                                                                       
alias lsd="ll | egrep '^d' | awk '{print $9}'"                                                                           
alias lla="ls -latr"                                                                                                     
alias psef="ps -eaf | grep "                                                                                             
alias tkdiff="/home/atirumal/bin/tkdiff"                                                                                 
export PS1="[\\h:\\w]-$ "
